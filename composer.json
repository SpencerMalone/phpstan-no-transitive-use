{
    "name": "spencermalone/phpstan-no-transitive-use",
    "description": "PHPStan extension to disallow transitive dependency use",
    "type": "phpstan-extension",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "phpstan/phpstan": "^2",
        "symfony/polyfill-php80": "^1.32",
        "nikic/php-parser": "^4|^5"
    },
    "autoload": {
        "psr-4": {
            "SpencerMalone\\NoTransitiveUse\\": "src/"
        }
    },
    "extra": {
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10|^11|^12"
    }
}
